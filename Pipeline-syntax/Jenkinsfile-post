pipeline
{
  agnt { label 'redhat-node' }
  stages
  {
    stage('first')
    {
      steps
      {
        echo "this is 1st step"
      }
    }
  }
    post {
      always
      {
        echo "this is independent of status of job"
      }
changed
{
echo "you will see it if there is a change in status from previous run"
}
  }
}
