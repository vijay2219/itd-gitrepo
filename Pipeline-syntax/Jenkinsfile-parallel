pipeline
{
  agent none
  stages {
    stage('parallel')
    {
      parallel
      {
        stage('first')
        {
agent any
          echo "this is 1st satge to run in parallel"
        }
        stage('second')
        {
         agent { label 'redhat-node'} 
echo "this is 2nd satge to run in parallel"
      }
    }
  }
}
}
